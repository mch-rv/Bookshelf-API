{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "import/no-extraneous-dependencies": "off",
        "no-unused-vars":"off",
        "no-prototype-builtins":"off",
        "implicit-arrow-linebreak":"off",
        "no-shadow":"off"
    }
}
